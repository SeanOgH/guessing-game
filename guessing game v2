import  random

number = random.randint(1,10)
print(number)

counter = 0

while counter < 5 :
    
    guess = int(input(" enter a number between 1 and 10 :  "))
    if guess == number:
        print ("correct")
        break
    elif guess < number :
        print ("too low")
    else :
        print ("too high")
        
        counter = counter + 1
        
print("Goodbye")
