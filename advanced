import random
personname = input("what is your name :  ")
print ("Hi" ,  personname")



number = random.randint (1,10)

guess = int (input (" Enter a number between 1 and 10 :  "))

if guess == number :
     print (" Your guess was correct ")
     print (" Well done! ")
     print (" Goodbye ")
elif guess < number :
    print (" Unlucky try again")
    print (" Too low")
   
    
elif guess > number :
         print (" Unlucky try again")
         print (" Too high")
